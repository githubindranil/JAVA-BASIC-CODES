import java.util.*;
public class JavaApplication23 {

    
    public static void main(String[] args) {
       
        Scanner sc=new Scanner(System.in);
        int n;
        System.out.println("Enter the number you need to multiply upto 10");
        n=sc.nextInt();
        for(int i=1;i<=10;i++)
        {
            System.out.println(n+"*"+i+"="+n*i);
        }
        
        
    }
    
}
